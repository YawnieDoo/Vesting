    function createTabs() {
      const tabs = document.getElementById('tabs');
      data.forEach((d, i) => {
        const btn = document.createElement('button');
        btn.className = 'tab-button';
        btn.textContent = d.title;
        btn.onclick = () => switchTab(d.id);
        tabs.appendChild(btn);
      });
    }

    function createContent() {
      const container = document.getElementById('content');
      data.forEach(d => {
        const div = document.createElement('div');
        div.id = d.id;
        div.className = 'tab-content';
        div.innerHTML = `
          <div class="vesting-section">
            <div class="vesting-card">
              <h2>${d.title} Vesting Schedule</h2>
              <p><strong>Wallet Address:</strong></p>
              <div style="display:flex;flex-wrap:wrap;align-items:center;gap:10px;margin-bottom:12px;">
                <code onclick="copyToClipboard('${d.address}')">${d.address}</code>
                <a class="simple-link-button" href="${d.view}" target="_blank">View on SolScan</a>
              </div>
              <p><strong>Allocated Tokens:</strong> ${d.allocation}</p>
              <p><strong>Vesting Platform:</strong> <a href="${d.streamflow}" target="_blank">${d.platform}</a></p>
              <p><strong>Contract Type:</strong> ${d.contract}</p>
              <p><strong>Purpose:</strong><br>${d.purpose}</p>
              <table class="vesting-table">
                <tr><th>Parameter</th><th>Details</th></tr>
                <tr><td>Cliff Period</td><td>${d.schedule[0]}</td></tr>
                <tr><td>Vesting Type</td><td>${d.schedule[1]}</td></tr>
                <tr><td>Vesting Duration</td><td>${d.schedule[2]}</td></tr>
                <tr><td>Total Lock Period</td><td>${d.schedule[3]}</td></tr>
                <tr><td>Unlock Rate</td><td>${d.schedule[4]} tokens per month</td></tr>
              </table>
              <p><strong>Unlock Schedule:</strong><br>${d.unlock}</p>
              <div class="countdown">
                <p><strong>Next Unlock In:</strong> <span id="next-${d.id}"></span></p>
                <p><strong>Fully Unlocked In:</strong> <span id="final-${d.id}"></span></p>
              </div>
              <div class="progress-wrap">
                <p><strong>Vesting Progress</strong></p>
                <div class="progress-bar"><div class="progress-fill" id="bar-${d.id}"></div></div>
              </div>
              <div class="button-wrap">
                <a href="${d.proof}" target="_blank">View Proof of Lock</a>
              </div>
            </div>
          </div>
        `;
        container.appendChild(div);
      });
    }

    function switchTab(id) {
      document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
      document.getElementById(id).classList.add('active');
    }

    function copyToClipboard(text) {
      navigator.clipboard.writeText(text).then(() => alert("Wallet address copied to clipboard!"));
    }

    function formatTime(ms) {
      const d = Math.floor(ms / 864e5), h = Math.floor(ms / 36e5) % 24,
            m = Math.floor(ms / 6e4) % 60, s = Math.floor(ms / 1e3) % 60;
      return `${d}d ${h}h ${m}m ${s}s`;
    }

    function updateCountdowns() {
      const now = new Date();
      data.forEach(d => {
        const next = new Date(d.next + "T00:00:00Z"), final = new Date(d.final + "T00:00:00Z");
        const toNext = next - now, toFinal = final - now, total = final - next, elapsed = now - next;
        document.getElementById("next-"+d.id).textContent = toNext > 0 ? formatTime(toNext) : "Already unlocked";
        document.getElementById("final-"+d.id).textContent = toFinal > 0 ? Math.floor(toFinal / 864e5) + " days" : "Fully unlocked";
        const pct = Math.min(Math.max((elapsed / total) * 100, 0), 100);
        document.getElementById("bar-"+d.id).style.width = `${pct}%`;
      });
    }

    createTabs();
    createContent();
    switchTab(data[0].id);
    updateCountdowns();
    setInterval(updateCountdowns, 1000);
  </script>
</body>
</html>
